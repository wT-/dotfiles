{
    "plugins": ["git", "hg", "ssh", "z", "aliases", "virtualenv"],
    "prompt": [
        ["darkred", "", ""],
        ["white", "", "$virtualenv"],
        ["green", "", " $path"],
        ["", "", " $git_branch"],
        ["", "", " $git_local_state"],
        ["", "", " $git_remote_state"],
        ["yellow", "", " $([text.encoding]::utf8.getstring((206,187)))"]
    ],
    "git": {
        "prompt_unstaged": "*",
        "prompt_staged": "+",
        "prompt_stash": "$",
        "prompt_untracked": "%",
        "prompt_remote_push": ">",
        "prompt_remote_pull": "<",
        "prompt_remote_same": "="
    },
    "hg": {
        "prompt_dirty": "*"
    }
}
